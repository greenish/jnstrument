{
  "name": "jnstrument",
  "version": "2.0.0",
  "main": "lib/babel-plugin/index.js",
  "dependencies": {
    "pino": "4.6.0"
  },
  "scripts": {
    "postinstall": "node -e \"var s='../.', d='./node_modules/jnstrument',fs=require('fs'); !fs.existsSync(d) && fs.symlinkSync(s,d,'dir');\"",
    "preinstall": "rm -R ./node_modules/jnstrument",
    "js:build": "rm -R compiled; ./node_modules/.bin/babel test.js --out-dir compiled --copy-files"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-register": "6.24.1"
  }
}
